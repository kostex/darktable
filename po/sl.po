msgctxt "preferences"
msgid "bilinear"
msgstr "bilinearni"

#: ../build/src/preferences_gen.h:3055
msgctxt "preferences"
msgid "bicubic"
msgstr "bikubični"

#: ../build/src/preferences_gen.h:3060
