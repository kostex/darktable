#: ../build/src/preferences_gen.h:3864 ../build/src/preferences_gen.h:3987
msgctxt "preferences"
msgid "TRUE"
msgstr "VRAI"

#: ../build/src/preferences_gen.h:2837
msgid "this option toggles the behavior of shift clicking in darkroom mode"
msgstr "cette option échange le comportement du shift-clic de la chambre noire"

#: ../build/src/preferences_gen.h:2845
msgid "expand the module when it is activated, and collapse it when disabled"
msgstr "développer le module lorsqu'activé, et réduire lorsque désactivé"

#: ../build/src/preferences_gen.h:2857
msgid ""
"this option allows to expand or collapse automatically the module when it is "
"enabled or disabled."
msgstr ""
"permet de développer ou réduire automatiquement un module lorsqu'il est "
"activé ou désactivé."

#: ../build/src/preferences_gen.h:2865
msgid "scroll to darkroom modules when expanded/collapsed"
msgstr ""
"positionne les modules de la chambre noire lorsque qu'ils sont dépliés/"
"repliés"

#: ../build/src/preferences_gen.h:2885
msgid "border around image in darkroom mode"
msgstr "bordure autour de l'image dans la chambre noire"

#: ../build/src/preferences_gen.h:2904
msgid ""
"process the image in darkroom mode with a small border. set to 0 if you "
"don't want any border."
msgstr ""
"affiche l'image dans la chambre noire avec une petite bordure. positionner à "
"0 pour supprimer la bordure."

#: ../build/src/preferences_gen.h:2912
msgid "map / geolocalisation"
msgstr "carte / géolocalisation"

#: ../build/src/preferences_gen.h:2923
msgid "only draw images on map that are currently collected and filtered"
msgstr "affiche sur la carte uniquement les images filtrées de la collection"

#: ../build/src/preferences_gen.h:2935
msgid ""
"use the current filter settings to select the geotagged images drawn on the "
"map, i.e. limit the images drawn to the current filmstrip. this limits the "
"number of images drawn and thus reduces the time needed to draw the images."
msgstr ""
"utilise le filtre courant pour l'affichage des images géolocalisées sur la "
"carte. i.e. limite le nombre d'images affichées au bandeau courant. cela "
"donne de meilleurs performances en limitant le images affichées sur la carte."

#: ../build/src/preferences_gen.h:2943
msgid "maximum number of images drawn on map"
msgstr "nombre maximum d'images affichées sur la carte"

#: ../build/src/preferences_gen.h:2962
msgid ""
"the maximum number of geotagged images drawn on the map. increasing this "
"number can slow drawing of the map down."
msgstr ""
"nombre maximum d'images géolocalisées affichées sur la carte. augmenter ce "
"nombre peut ralentir l'affichage de la carte."

#: ../build/src/preferences_gen.h:2970
msgid "pretty print the image location"
msgstr "affichage plus clair de la localisation d'image"

#: ../build/src/preferences_gen.h:2982
msgid ""
"show a more readable representation of the location in the image information "
"module"
msgstr ""
"affiche une représentation plus lisible de la localisation dans le module "
"'informations de l'image'"

#: ../build/src/preferences_gen.h:2990
msgid "security"
msgstr "sécurité"

#: ../build/src/preferences_gen.h:3001
msgid "ask before removing images from database"
msgstr "demander avant d'enlever des images"

#: ../build/src/preferences_gen.h:3013
msgid "always ask the user before any image is removed from DB."
msgstr "toujours demander avant d'enlever une image de la base de données."

#: ../build/src/preferences_gen.h:3021
msgid "ask before erasing images from disk / discarding history stack"
msgstr "demander avant de supprimer des images / supprimer l'historique"

#: ../build/src/preferences_gen.h:3033
msgid ""
"always ask the user before any image file is deleted / history stack is "
"discarded."
msgstr ""
"toujours demander avant de supprimer un fichier image / supprimer "
"l'historique."

#: ../build/src/preferences_gen.h:3041
msgid "send files to trash when erasing images"
msgstr "déplace les fichiers vers la poubelle lors de l'effacement"

#: ../build/src/preferences_gen.h:3053
msgid ""
"send files to trash instead of permanently deleting files on system that "
"supports it"
msgstr ""
"déplace les fichiers vers la poubelle au lieu de définitivement les "
"supprimer sur les système qui le supporte"

#: ../build/src/preferences_gen.h:3061
msgid "ask before moving images from film roll folder"
msgstr "demander avant de déplacer des images vers un autre dossier"

#: ../build/src/preferences_gen.h:3073
msgid "always ask the user before any image file is moved."
msgstr "toujours demander avant de déplacer une image."

#: ../build/src/preferences_gen.h:3081
msgid "ask before copying images to new film roll folder"
msgstr "demander avant de copier des images vers un nouveau dossier"

#: ../build/src/preferences_gen.h:3093
msgid "always ask the user before any image file is copied."
msgstr "toujours demander avant de copier une image."

#: ../build/src/preferences_gen.h:3101
msgid "ask before removing empty folders"
msgstr "demander avant d'enlever les répertoires vides"

#: ../build/src/preferences_gen.h:3113
msgid ""
"always ask the user before removing any empty folder. this can happen after "
"moving or deleting images."
msgstr ""
"toujours demander à l'utilisateur avant de supprimer un répertoire vide. "
"ceci peut arriver lors du déplacement ou de la suppression d'image."

#: ../build/src/preferences_gen.h:3121
msgid "ask before deleting a tag"
msgstr "demander avant de supprimer un mot-clé"

#: ../build/src/preferences_gen.h:3140
msgid "ask before deleting a style"
msgstr "demander avant de supprimer un style"

#: ../build/src/preferences_gen.h:3159 ../build/src/preferences_gen.h:4061
msgid "miscellaneous"
msgstr "divers"

#: ../build/src/preferences_gen.h:3170
msgid "width of the side panels in pixels"
msgstr "taille des panneaux latéraux en pixels"

#: ../build/src/preferences_gen.h:3189
msgid "(needs a restart)"
msgstr "(nécessite un redémarrage)"

#: ../build/src/preferences_gen.h:3197
msgid "waiting time between each picture in slideshow"
msgstr "temporisation entre chaque image du diaporama"

#: ../build/src/preferences_gen.h:3223
msgid "do not show april 1st game"
msgstr "ne jamais afficher le jeu du 1er avril"

#: ../build/src/preferences_gen.h:3242
msgid "number of folder levels to show in lists"
msgstr "nombre de niveaux de dossiers à afficher"

#: ../build/src/preferences_gen.h:3261
msgid ""
"the number of folder levels to show in film roll names, starting from the "
"right"
msgstr ""
"nombre de niveaux de dossier à afficher dans les noms, en partant de la "
"droite"

#: ../build/src/preferences_gen.h:3269
