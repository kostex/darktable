#: ../build/src/preferences_gen.h:1935 ../src/libs/tools/lighttable.c:74
#: ../src/views/lighttable.c:200
msgid "lighttable"
msgstr "lysbord"

#: ../build/src/preferences_gen.h:1946
msgid "don't use embedded preview JPEG but half-size raw"
msgstr "bruk halv størrelse raw i stedet for innebygget JPEG"

#: ../build/src/preferences_gen.h:1958
msgid ""
"check this option to not use the embedded JPEG from the raw file but process "
"the raw data. this is slower but gives you color managed thumbnails."
msgstr ""
"velg dette for å bruke RAW-data i stedet for den innebygde JPEG-en. Denne "
"metoden er tregere, men resulterer i fargestyrte miniatyrbilder."

#: ../build/src/preferences_gen.h:1966
msgid "high quality thumb processing from size"
msgstr "høyoppløselig miniatyrbilde fra størrelse"

#: ../build/src/preferences_gen.h:1977 ../build/src/preferences_gen.h:3125
msgctxt "preferences"
msgid "always"
msgstr "alltid"

#: ../build/src/preferences_gen.h:1982
msgctxt "preferences"
msgid "small"
msgstr "liten"

#: ../build/src/preferences_gen.h:1987
msgctxt "preferences"
msgid "VGA"
msgstr "VGA"

#: ../build/src/preferences_gen.h:1992 ../build/src/preferences_gen.h:2032
msgctxt "preferences"
msgid "720p"
msgstr "720p"

#: ../build/src/preferences_gen.h:1997
msgctxt "preferences"
msgid "1080p"
msgstr "1080p"

